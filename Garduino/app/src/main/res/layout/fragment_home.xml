    <com.jjoe64.graphview.GraphView
        android:layout_width="match_parent"
        android:layout_height="200dip"
        android:id="@+id/graph"
        android:layout_alignBottom="@+id/text3"
        android:layout_marginBottom="120dp"/>
</RelativeLayout>
